{
    "name": "offlinets",
    "version": "3.2.4-2",
    "description": "ping a service to check if it is up",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "node node_modules/nyc/bin/nyc --reporter=text node_modules/mocha/bin/_mocha --require ts-node/register -u tdd --timeout 999999 --colors ./test/offlineTsSpec.ts",
        "build": "node ./node_modules/gulp/bin/gulp build",
        "demo": "node ./node_modules/gulp/bin/gulp demo && ts-node ./demo/Server.ts",
        "prepare": "npm run build",
        "prepublishOnly": "npm test"
    },
    "files": [
        "dist/**/*"
    ],
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/InTimeTecGitHub/offlineTs"
    },
    "author": "Akhil Sasidharan",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/express": "^4.16.1",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.21",
        "@types/sinon": "^7.0.5",
        "body-parser": "^1.20.3",
        "browserify": "^16.2.3",
        "chai": "^4.2.0",
        "express": "^4.21.0",
        "gulp": "^4.0.2",
        "gulp-typescript": "^5.0.0",
        "gulp-uglifyes": "^0.2.1",
        "mocha": "^5.2.0",
        "nyc": "^15.1.0",
        "sinon": "^7.2.3",
        "ts-node": "^8.0.2",
        "tsify": "^4.0.1",
        "typescript": "^3.3.3",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    },
    "dependencies": {},
    "keywords": [
        "offlineTs",
        "offline",
        "offlinejs",
        "down",
        "servicedown",
        "ping"
    ]
}
